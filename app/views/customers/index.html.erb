<h1>Our Customers</h1>
<!-- start -->
    <%= form_tag customers_path, multipart: true do %>
  <%= file_field_tag :leads %>
  <%= submit_tag "Import Leads" %>
<% end %>
<!-- end --> 
<ul>
<% @customers.each do |customer| %>
  <li><%= "#{customer.first_name} #{customer.last_name} - #{customer.email}" %></li>
<% end %>
</ul>
